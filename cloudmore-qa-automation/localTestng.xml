<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestSuite-Cloudmore Automation Assignment" thread-count="1" parallel="tests">

    <listeners>
        <listener class-name="com.cloudmore.listener.TestRetryAnalyzerListener"/>
    </listeners>

    <!-- TO generate Extent Report for UI Automation Please run TestNG.XML Otherwise cucumber Report only executed -->
